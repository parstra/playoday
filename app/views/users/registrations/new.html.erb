<section class="signup fancy_form">
<h2>Sign up</h2>

<%= form_for resource, url: registration_path(resource_name) do |f| %>
<%= render partial: "application/notice", object: current_user %>
  <%= devise_error_messages! %>

  <div>
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>

  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>

  <div>
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>
  </div>

  <div>
    <%= f.submit "Sign up", :class => "base_button" %>
  </div>
<% end %>
</section>

